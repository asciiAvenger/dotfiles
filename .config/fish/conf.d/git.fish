# Git aliases
alias ga='git add'
alias gaa='git add --all'
alias gd='git diff'
alias gp='git push'
alias gl='git pull'
alias glo='git log --oneline --graph --decorate'
alias gm='git merge'
alias gss='git status --short'
alias gc='git commit --verbose'
alias gf='git fetch --all'
alias gfp='git fetch --all --prune'
alias gsw='git switch'

